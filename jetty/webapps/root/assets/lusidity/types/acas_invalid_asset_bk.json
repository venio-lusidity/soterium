{
  "name": "ACAS Invalid Asset",
  "zones": [
    {
      "layout": "zone",
      "zone": ".menu-bar",
      "styles": ["/assets/lusidity/css/entity.css",
        "/assets/lusidity/plugins/css/treeView.css",
        "/assets/lusidity/plugins/css/tableView.css"],
      "plugins": [
        "/assets/lusidity/plugins/pGrid_v1.js",
        "/assets/lusidity/plugins/pGridViewer.js",
        "/assets/lusidity/plugins/pVulnHistory.js",
        "/assets/lusidity/plugins/pVulnerabilities_v2.js",
        "/assets/lusidity/plugins/jquery.vulnDetails.js",
        "/assets/lusidity/plugins/jquery.iavmDetails.js",
        "/assets/lusidity/plugins/jquery.osIcon.js",
        "/assets/lusidity/plugins/jquery.semanticType.js",
        "/assets/lusidity/plugins/jquery.image.js",
        "/assets/lusidity/plugins/jquery.nrt.metrics.js",
        "/assets/lusidity/plugins/jquery.treeViewCommon.js",
        "/assets/lusidity/plugins/jquery.assetMenuBar.js",
        "/assets/lusidity/plugins/jquery.dataList.js",
        "/assets/lusidity/plugins/jquery.networkAdapters.js",
        "/assets/lusidity/plugins/jquery.vulnHistory.js",
        "/assets/lusidity/plugins/jquery.softwareCompliant_v2.js",
        "/assets/lusidity/plugins/jquery.workflow.js",
        "/assets/lusidity/plugins/jquery.poam.js",
        "/assets/lusidity/plugins/jquery.vertexResolver.js",
        "/assets/lusidity/plugins/jquery.importerMatchedOn.js"
      ],
      "schemas": [
        {
          "layout": "plugin",
          "property": "/",
          "once": true,
          "name": "assetMenuBar",
          "plugin":{
            "enabled": true
          }
        }
      ]
    },
    {
      "layout": "zone",
      "zone": ".panel-left",
      "schemas": [
        {
          "layout": "html",
          "type": "div",
          "schemas": [
            {
              "layout": "data",
              "property": "title",
              "type": "h4",
              "css": { "display": "block", "text-align": "center" }
            }
          ]
        },
        {
          "layout": "plugin",
          "property": "/",
          "type": "div",
          "name": "osIcon",
          "plugin":{
            "display": "tile",
            "fontSize": "84px",
            "cssFiles": [
            ]
          }
        },
        {
          "layout": "plugin",
          "property": "instanceOf",
          "type": "div",
          "name": "semanticType",
          "css": { "display": "block", "text-align": "center" },
          "plugin":{
            "image": false,
            "cssFiles": [
              "/assets/lusidity/plugins/css/semanticType.css"
            ]
          }
        },
        {
          "layout": "html",
          "type": "div",
          "css": {"padding": "0 5px 0 5px"},
          "schemas": [
            {
              "layout": "html",
              "type": "div",
              "schemas": [
                {
                  "layout": "html",
                  "html": "<div class=\"data-label\">Deprecated:</div>"
                },
                {
                  "layout": "data",
                  "property": "deprecated",
                  "type": "div",
                  "cls": "data-value"
                }
              ]
            },
            {
              "layout": "html",
              "type": "div",
              "schemas": [
                {
                  "layout": "html",
                  "html": "<div class=\"data-label\">Host:</div>"
                },
                {
                  "layout": "data",
                  "property": "hostname",
                  "type": "div",
                  "cls": "data-value"
                }
              ]
            },
            {
              "layout": "html",
              "type": "div",
              "schemas": [
                {
                  "layout": "html",
                  "html": "<div class=\"data-label\">Domain:</div>"
                },
                {
                  "debug": true,
                  "layout": "data",
                  "property": "domainName",
                  "type": "div",
                  "cls": "data-value"
                }
              ]
            },
            {
              "layout": "html",
              "type": "div",
              "schemas": [
                {
                  "layout": "html",
                  "html": "<div class=\"data-label\">Manufacturer:</div>"
                },
                {
                  "layout": "data",
                  "property": "manufacturer",
                  "type": "div",
                  "cls": "data-value"
                }
              ]
            },
            {
              "layout": "html",
              "type": "div",
              "schemas": [
                {
                  "layout": "html",
                  "html": "<div class=\"data-label\">Make:</div>"
                },
                {
                  "layout": "data",
                  "property": "make",
                  "type": "div",
                  "cls": "data-value"
                }
              ]
            },
            {
              "layout": "html",
              "type": "div",
              "schemas": [
                {
                  "layout": "html",
                  "html": "<div class=\"data-label\">Model:</div>"
                },
                {
                  "layout": "data",
                  "property": "modelNumber",
                  "type": "div",
                  "cls": "data-value"
                }
              ]
            },
            {
              "layout": "html",
              "type": "div",
              "schemas": [
                {
                  "layout": "html",
                  "html": "<div class=\"data-label\">Serial Number:</div>"
                },
                {
                  "layout": "data",
                  "property": "serialNumber",
                  "type": "div",
                  "cls": "data-value"
                }
              ]
            },
            {
              "layout": "html",
              "type": "div",
              "schemas": [
                {
                  "layout": "html",
                  "html": "<div class=\"data-label\">Barcode:</div>"
                },
                {
                  "layout": "data",
                  "property": "barCode",
                  "type": "div",
                  "cls": "data-value"
                }
              ]
            }
          ]
        },
        {
          "layout": "html",
          "type": "div",
          "css": {"margin-left": "5px"},
          "schemas": [
            {
              "layout": "html",
              "html": "<div class=\"data-label\">Created When:</div>"
            },
            {
              "layout": "data",
              "property": "createdWhen",
              "dataType": "date",
              "format": "j\\-M\\-Y\\, g\\:i a",
              "type": "div",
              "maxItems": 1,
              "cls": "data-value"
            }
          ]
        },
        {
          "layout": "html",
          "type": "div",
          "schemas": [
            {
              "layout": "plugin",
              "property": "/",
              "type": "div",
              "name": "dataList",
              "css": { "display": "block"},
              "plugin":{
                "hasItemCounter": false,
                "glyph": "glyphicons glyphicons-group",
                "title": "Host History",
                "property": "/system/primitives/text_value/hosts",
                "sort": {"createdWhen":"desc"},
                "limit": 10,
                "blocked": true,
                "properties":[
                  {
                    "label": "Host",
                    "property": "value"
                  },
                  {
                    "label": "Source",
                    "property": "label"
                  },
                  {
                    "label": "Modified When",
                    "property": "modifiedWhen",
                    "dataType": "dateOnly"
                  }
                ]
              }
            }
          ]
        },
        {
          "layout": "html",
          "type": "div",
          "schemas": [
            {
              "layout": "plugin",
              "property": "/",
              "type": "div",
              "name": "dataList",
              "css": { "display": "block"},
              "plugin":{
                "hasItemCounter": false,
                "glyph": "glyphicons glyphicons-group",
                "title": "System Name",
                "property": "/",
                "query": {
                  "fn": "deviceToEnclave",
                  "limit": 1
                },
                "limit": 10,
                "properties":[
                  {
                    "property": "title",
                    "linked": true
                  }
                ]
              }
            }
          ]
        },
        {
          "layout": "html",
          "type": "div",
          "schemas": [
            {
              "layout": "plugin",
              "property": "/",
              "type": "div",
              "name": "dataList",
              "css": { "display": "block"},
              "plugin":{
                "hasItemCounter": false,
                "glyph": "glyphicons glyphicons-group",
                "title": "Owned By",
                "property": "/organization/organization/ownedBy",
                "limit": 10,
                "properties":[
                  {
                    "property": "title",
                    "linked": true,
                    "params": {"et_view": "owned"}
                  }
                ]
              }
            }
          ]
        },
        {
          "layout": "html",
          "type": "div",
          "schemas": [
            {
              "layout": "plugin",
              "property": "/",
              "type": "div",
              "name": "dataList",
              "css": { "display": "block"},
              "plugin":{
                "hasItemCounter": false,
                "glyph": "glyphicons glyphicons-group",
                "title": "Managed By",
                "property": "/organization/organization/managedBy",
                "limit": 10,
                "properties":[
                  {
                    "property": "title",
                    "linked": true
                  }
                ]
              }
            }
          ]
        },
        {
          "layout": "html",
          "type": "div",
          "schemas": [
            {
              "layout": "plugin",
              "property": "/",
              "type": "div",
              "name": "dataList",
              "css": { "display": "block"},
              "plugin":{
                "hasItemCounter": false,
                "glyph": "glyphicons glyphicons-map",
                "title": "Location",
                "property": "/location/location/places",
                "limit": 10,
                "properties":[
                  {
                    "property": "title",
                    "linked": true
                  }
                ]
              }
            }
          ]
        },
        {
          "layout": "html",
          "type": "div",
          "schemas": [
            {
              "layout": "plugin",
              "property": "/",
              "type": "div",
              "name": "dataList",
              "css": { "display": "block"},
              "plugin":{
                "hasItemCounter": false,
                "glyph": "glyphicons glyphicons-engineering-networks",
                "title": "Central Processing Units",
                "property": "/electronic/central_processing_unit/cpu",
                "limit": 10,
                "properties":[
                  {
                    "label": "Make",
                    "property": "make"
                  },
                  {
                    "label": "Model Number",
                    "property": "modelNumber"
                  },
                  {
                    "label": "Serial Number",
                    "property": "serialNumber"
                  },
                  {
                    "label": "Speed",
                    "property": "speed"
                  }
                ]
              }
            }
          ]
        },
        {
          "layout": "html",
          "type": "div",
          "schemas": [
            {
              "layout": "plugin",
              "property": "/",
              "type": "div",
              "name": "networkAdapters",
              "css": { "display": "block"},
              "plugin":{
                "hasItemCounter": false,
                "glyph": "glyphicons glyphicons-engineering-networks",
                "title": "Network Adapters",
                "limit": 10,
                "blocked": true,
                "sort": {"modifiedWhen":"desc"},
                "boldFirst": true,
                "properties":[
                  {
                    "label": "MAC Address",
                    "property": "macAddress"
                  },
                  {
                    "label": "IP Address",
                    "property": "ipAddress"
                  },
                  {
                    "label": "Seen on",
                    "property": "modifiedWhen",
                    "dataType": "date",
                    "format": "j\\-M\\-Y\\, g\\:i a"
                  }
                ]
              }
            }
          ]
        },
        {
          "layout": "html",
          "type": "div",
          "schemas": [
            {
              "layout": "plugin",
              "property": "/",
              "type": "div",
              "name": "dataList",
              "css": { "display": "block", "word-break":"break-word"},
              "plugin":{
                "hasItemCounter": false,
                "glyph": "glyphicons glyphicons-keys",
                "title": "Identifiers",
                "property": "/system/primitives/uri_value/identifiers",
                "limit": 10,
                "blocked": true,
                "properties":[
                  {
                    "label": "From",
                    "property": "fn::classToName::label"
                  },
                  {
                    "label": "Identifier",
                    "property": "value"
                  }
                ]
              }
            }
          ]
        },
        {
          "layout": "html",
          "type": "div",
          "schemas": [
            {
              "layout": "plugin",
              "property": "/",
              "type": "div",
              "name": "dataList",
              "css": { "display": "block", "word-break":"break-word"},
              "plugin":{
                "hasItemCounter": false,
                "glyph": "glyphicons glyphicons-keys",
                "title": "Other Identifiers",
                "property": "/system/primitives/uri_value/volatileIdentifiers",
                "limit": 10,
                "blocked": true,
                "properties":[
                  {
                    "label": "From",
                    "property": "label"
                  },
                  {
                    "label": "Identifier",
                    "property": "value"
                  }
                ]
              }
            }
          ]
        }
      ]
    },
    {
      "layout": "zone",
      "zone": ".panel-middle",
      "cls": "no-border",
      "schemas": [
        {
          "layout": "html",
          "type": "div",
          "cls": "full-min",
          "attr": {"data-oh": 7},
          "schemas":[
            {
              "layout": "data",
              "property": "/",
              "type": "div",
              "schemas":[
                {
                  "property": "/system/primitives/raw_string/descriptions.results",
                  "schemas":[
                    {
                      "layout": "data",
                      "type": "p",
                      "property": "value"
                    }
                  ]
                }
              ]
            },
            {
              "layout": "html",
              "type": "ul",
              "cls": "nav nav-tabs",
              "schemas":[
                {
                  "layout": "html",
                  "type": "li",
                  "cls": "active",
                  "schemas":[
                    {
                      "layout": "html",
                      "type": "a",
                      "text": "Findings",
                      "attr": {"data-toggle": "tab", "href": "#tab-mitigations"}
                    }
                  ]
                },
                {
                  "layout": "html",
                  "type": "li",
                  "schemas":[
                    {
                      "layout": "html",
                      "type": "a",
                      "text": "Software",
                      "attr": {"data-toggle": "tab", "href": "#tab-software"}
                    }
                  ]
                },
                {
                  "layout": "html",
                  "type": "li",
                  "schemas":[
                    {
                      "layout": "html",
                      "type": "a",
                      "text": "HBSS Baseline Compliance",
                      "attr": {"data-toggle": "tab", "href": "#tab-compliance"}
                    }
                  ]
                },
                {
                  "layout": "html",
                  "type": "li",
                  "schemas":[
                    {
                      "layout": "html",
                      "type": "a",
                      "text": "Workflows",
                      "attr": {"data-toggle": "tab", "href": "#tab-workflow-history"},
                      "css": {"display": "none"}
                    }
                  ]
                }
              ]
            },
            {
              "layout": "html",
              "type": "div",
              "cls": "tab-content",
              "schemas":[
                {
                  "layout": "html",
                  "type": "div",
                  "cls": "tab-pane active",
                  "attr": {"id": "tab-mitigations"},
                  "schemas": [
                    {
                      "layout": "html",
                      "type": "ul",
                      "cls": "nav nav-tabs tab-under",
                      "schemas":[
                        {
                          "layout": "html",
                          "type": "li",
                          "cls": "active",
                          "schemas":[
                            {
                              "layout": "html",
                              "type": "a",
                              "text": "Scan Failures",
                              "attr": {"data-toggle": "tab", "id": "tab-mitigations-1-tab", "href": "#tab-mitigations-1"}
                            }
                          ]
                        },
                        {
                          "layout": "html",
                          "type": "li",
                          "schemas":[
                            {
                              "layout": "html",
                              "type": "a",
                              "text": "Findings History",
                              "attr": {"data-toggle": "tab", "id": "tab-mitigations-3-tab", "href": "#tab-mitigations-3"}
                            }
                          ]
                        },
                        {
                          "layout": "html",
                          "type": "li",
                          "schemas":[
                            {
                              "layout": "html",
                              "type": "a",
                              "text": "Importers",
                              "attr": {"data-toggle": "tab", "href": "#tab-importer-history"}
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "layout": "html",
                      "type": "div",
                      "cls": "tab-content no-border",
                      "schemas":[
                        {
                          "layout": "html",
                          "type": "div",
                          "cls": "tab-pane no-border active",
                          "css": {"height": "100%"},
                          "attr": {
                            "id": "tab-mitigations-1"
                          },
                          "schemas": [
                            {
                              "layout": "plugin",
                              "property": "/",
                              "type": "div",
                              "name": "pVulnerabilities",
                              "fill": true,
                              "plugin":{
                                "title": "Findings by Priority",
                                "glyph": "glyphicons-bug",
                                "active": true,
                                "adjustHeight": 0,
                                "hInherit": false,
                                "offset": {
                                  "parent": 0,
                                  "table": 0,
                                  "onResize": -10
                                },
                                "initOnClick": "tab-mitigations-1-tab"
                              }
                            }
                          ]
                        },
                        {
                          "layout": "html",
                          "type": "div",
                          "cls": "tab-pane no-border",
                          "css": {"height": "100%"},
                          "attr": {
                            "id": "tab-mitigations-3"
                          },
                          "schemas": [
                            {
                              "layout": "plugin",
                              "property": "/",
                              "type": "div",
                              "name": "pVulnHistory",
                              "fill": true,
                              "css": {
                                "display": "block"
                              },
                              "plugin": {
                                "glyph": "glyphicons glyphicons-info-sign",
                                "title": "History",
                                "limit": 100,
                                "adjustHeight": 0,
                                "hInherit": true,
                                "offset": {
                                  "parent": 0,
                                  "table": 0,
                                  "onResize": -10
                                },
                                "initOnClick": "tab-mitigations-3-tab"
                              }
                            }
                          ]
                        },
                        {
                          "layout": "html",
                          "type": "div",
                          "cls": "tab-pane no-border",
                          "attr": {
                            "id": "tab-importer-history"
                          },
                          "schemas": [
                            {
                              "layout": "plugin",
                              "property": "/",
                              "type": "div",
                              "name": "dataList",
                              "css": { "display": "block"},
                              "plugin":{
                                "glyph": "glyphicons glyphicons-info-sign",
                                "title": "Importers",
                                "property": "/technology/security/vulnerabilities/vulnerability_importer/importers",
                                "sort": {"role": "asc", "scannedOn": "desc"},
                                "properties":[
                                  {
                                    "label": "Source:",
                                    "property": "source"
                                  },
                                  {
                                    "label": "Importer:",
                                    "property": "fn::classToName::importer"
                                  },
                                  {
                                    "label": "Role:",
                                    "property": "role"
                                  },
                                  {
                                    "label": "Scanned On:",
                                    "property": "fn::toDefaultDate::scannedOn"
                                  }
                                ]
                              }
                            }
                          ]
                        }
                      ]
                    }
                  ]
                },
                {
                  "layout": "html",
                  "type": "div",
                  "cls": "tab-pane",
                  "attr": {"id": "tab-workflow-history"},
                  "schemas": [
                    {
                      "layout": "plugin",
                      "property": "/",
                      "type": "div",
                      "name": "workflow",
                      "css": { "display": "block"},
                      "plugin":{
                      }
                    }
                  ]
                },
                {
                  "layout": "html",
                  "type": "div",
                  "cls": "tab-pane tabbable sizable",
                  "attr": {"id": "tab-software"},
                  "schemas": [
                    {
                      "layout": "plugin",
                      "property": "/",
                      "type": "div",
                      "name": "dataList",
                      "css": { "display": "block"},
                      "plugin":{
                        "glyph": "glyphicons glyphicons-info-sign",
                        "title": "Applications",
                        "property": "/technology/software/applications",
                        "limit": 10000,
                        "properties":[
                          {
                            "property": "title",
                            "linked": true
                          },
                          {
                            "label": "Version:",
                            "property": "/technology/software_version/version.version"
                          }
                        ]
                      }
                    }
                  ]
                },
                {
                  "layout": "html",
                  "type": "div",
                  "cls": "tab-pane",
                  "attr": {"id": "tab-compliance"},
                  "schemas":[
                    {
                      "layout": "plugin",
                      "property": "/",
                      "type": "div",
                      "name": "softwareCompliant",
                      "css": { "display": "block"},
                      "plugin":{
                        "glyph": "glyphicons-check",
                        "title": "Software Compliance Policies",
                        "property": "/technology/software_compliance_policy/compliance",
                        "limit": 10000,
                        "properties":[
                          {
                            "property": "title",
                            "linked": true
                          },
                          {
                            "label": "CPE:",
                            "property": "cpe"
                          }
                        ]
                      }
                    }
                  ]
                }
              ]
            }
          ]
        }
      ]
    }
  ]
}