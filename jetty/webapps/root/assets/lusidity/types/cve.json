{
  "name": "CVE",
  "zones": [
    {
      "layout": "zone",
      "zone": ".menu-bar",
      "styles": ["/assets/lusidity/css/entity.css"],
      "plugins": [
        "/assets/lusidity/plugins/jquery.semanticType.js",
        "/assets/lusidity/plugins/jquery.image.js",
        "/assets/lusidity/plugins/jquery.nrt.metrics.js",
        "/assets/lusidity/plugins/jquery.dataList.js",
        "/assets/lusidity/plugins/jquery.baseMetrics.js",
        "/assets/lusidity/plugins/jquery.treeViewCommon.js"
      ],
      "schemas": [
      ]
    },
    {
      "layout": "zone",
      "zone": ".panel-left",
      "css": {"padding": "0 5px 0 5px"},
      "schemas": [
        {
          "layout": "html",
          "type": "div",
          "schemas": [
            {
              "layout": "data",
              "property": "vulnId",
              "type": "h5",
              "css": { "display": "block", "text-align": "center" }
            }
          ]
        },
        {
          "layout": "plugin",
          "property": "/",
          "type": "div",
          "cls": "cover-art center",
          "name": "image",
          "plugin":{
            "image": "/assets/img/types/cve.png",
            "cls": "med",
            "cssFiles": [
              "/assets/lusidity/plugins/css/image.css"
            ]
          }
        },
        {
          "layout": "plugin",
          "property": "instanceOf",
          "type": "div",
          "name": "semanticType",
          "css": { "display": "block", "text-align": "center" },
          "plugin":{
            "image": false,
            "cssFiles": [
              "/assets/lusidity/plugins/css/semanticType.css"
            ]
          }
        },
        {
          "layout": "html",
          "type": "div",
          "schemas": [
            {
              "layout": "html",
              "html": "<div class=\"data-label\">Published:</div>"
            },
            {
              "layout": "data",
              "property": "published",
              "dataType": "date",
              "format": "j\\-M\\-Y\\, g\\:i a",
              "type": "div",
              "maxItems": 1,
              "cls": "data-value"
            }
          ]
        },
        {
          "layout": "html",
          "type": "div",
          "schemas": [
            {
              "layout": "html",
              "html": "</br><div class=\"data-label\">System Information:</div>"
            }
          ]
        },
        {
          "layout": "html",
          "type": "div",
          "schemas": [
            {
              "layout": "html",
              "html": "<div class=\"data-label\">Created When:</div>"
            },
            {
              "layout": "data",
              "property": "createdWhen",
              "dataType": "date",
              "format": "j\\-M\\-Y\\, g\\:i a",
              "type": "div",
              "maxItems": 1,
              "cls": "data-value"
            }
          ]
        },
        {
          "layout": "html",
          "type": "div",
          "schemas": [
            {
              "layout": "html",
              "html": "<div class=\"data-label\">Modified When:</div>"
            },
            {
              "layout": "data",
              "property": "modifiedWhen",
              "dataType": "date",
              "format": "j\\-M\\-Y\\, g\\:i a",
              "type": "div",
              "maxItems": 1,
              "cls": "data-value"
            }
          ]
        }
      ]
    },
    {
      "layout": "zone",
      "zone": ".panel-middle",
      "cls": "no-border full-min",
      "schemas": [
        {
          "layout": "html",
          "type": "div",
          "attr": {"data-oh": 7},
          "schemas":[
            {
              "layout": "html",
              "type": "div",
              "schemas": [
                {
                  "layout": "data",
                  "property": "title",
                  "type": "div",
                  "cls": "data-value"
                }
              ]
            },
            {
              "layout": "data",
              "property": "/",
              "type": "div",
              "css": {"padding": "10px"},
              "schemas":[
                {
                  "property": "/system/primitives/raw_string/descriptions.results",
                  "schemas":[
                    {
                      "layout": "data",
                      "type": "p",
                      "property": "value"
                    }
                  ]
                }
              ]
            },
            {
              "layout": "html",
              "type": "ul",
              "cls": "nav nav-tabs",
              "schemas":[
                {
                  "layout": "html",
                  "type": "li",
                  "cls": "active",
                  "schemas":[
                    {
                      "layout": "html",
                      "type": "a",
                      "text": "Details",
                      "cls": "no-radius",
                      "attr": {"data-toggle": "tab", "href": "#tab-1"}
                    }
                  ]
                },
                {
                  "layout": "html",
                  "type": "li",
                  "schemas":[
                    {
                      "layout": "html",
                      "type": "a",
                      "text": "Related Security Center Plugins",
                      "cls": "no-radius",
                      "attr": {"data-toggle": "tab", "href": "#tab-2"}
                    }
                  ]
                },
                {
                  "layout": "html",
                  "type": "li",
                  "schemas":[
                    {
                      "layout": "html",
                      "type": "a",
                      "text": "Related IAVMs",
                      "cls": "no-radius",
                      "attr": {"data-toggle": "tab", "href": "#tab-3"}
                    }
                  ]
                }
              ]
            },
            {
              "layout": "html",
              "type": "div",
              "cls": "tab-content",
              "schemas":[
                {
                  "layout": "html",
                  "type": "div",
                  "cls": "tab-pane active",
                  "attr": {"id": "tab-1"},
                  "css": { "padding": "10px"},
                  "schemas":[
                    {
                      "layout": "html",
                      "html": "<h5><a href=\"http://nvd.nist.gov\" target=\"_blank\">Modified CVSS</a></h5>",
                      "schemas": [
                        {
                          "common":[
                            {
                              "property": "/technology/security/vulnerabilities/cve/cvss_metric/baseMetrics.generated",
                              "label": "Generated:",
                              "dataType": "date",
                              "format": "j\\-M\\-Y\\, g\\:i a"
                            },
                            {
                              "property": "/technology/security/vulnerabilities/cve/cvss_metric/baseMetrics.base",
                              "label": "Base Score:"
                            },
                            {
                              "property": "/technology/security/vulnerabilities/cve/cvss_metric/baseMetrics.accessVector",
                              "label": "Access Vector:"
                            },
                            {
                              "property": "/technology/security/vulnerabilities/cve/cvss_metric/baseMetrics.accessComplexity",
                              "label": "Access Complexity:"
                            },
                            {
                              "property": "/technology/security/vulnerabilities/cve/cvss_metric/baseMetrics.authentication",
                              "label": "Authentication:"
                            },
                            {
                              "property": "/technology/security/vulnerabilities/cve/cvss_metric/baseMetrics.confidentialityImpact",
                              "label": "Confidentiality Impact:"
                            },
                            {
                              "property": "/technology/security/vulnerabilities/cve/cvss_metric/baseMetrics.integrityImpact",
                              "label": "Integrity Impact:"
                            },
                            {
                              "property": "/technology/security/vulnerabilities/cve/cvss_metric/baseMetrics.availabilityImpact",
                              "label": "Availabiltiy Impact:"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "layout": "plugin",
                      "property": "/",
                      "type": "div",
                      "name": "dataList",
                      "css": { "display": "block"},
                      "plugin":{
                        "glyph": "glyphicons glyphicons-info-sign",
                        "hasHeader" :true,
                        "title": "References",
                        "property": "/system/primitives/uri_value/references",
                        "limit": 50,
                        "properties":[
                          {
                            "property": "value",
                            "linked": true,
                            "hrefProperty": "value"
                          }
                        ]
                      }
                    }
                  ]
                },
                {
                  "layout": "html",
                  "type": "div",
                  "cls": "tab-pane tabbable sizable",
                  "attr": {"id": "tab-2"},
                  "css": { "padding": "10px"},
                  "schemas": [
                    {
                      "layout": "plugin",
                      "property": "/",
                      "type": "div",
                      "name": "dataList",
                      "css": { "display": "block"},
                      "plugin":{
                        "fill": true,
                        "hasHeader": true,
                        "hasItemCounter": true,
                        "adjustHeight": 68,
                        "glyph": "glyphicons glyphicons-info-sign",
                        "title": "Security Center Plugins",
                        "property": "/technology/security/vulnerabilities/cve/cve/cves",
                        "direction": "in",
                        "limit": 100,
                        "properties":[
                          {
                            "property": "vulnId",
                            "linked": true
                          },
                          {
                            "property": "title"
                          },
                          {
                            "property": "published",
                            "label": "Published:",
                            "dataType": "date",
                            "format": "j\\-M\\-Y\\, g\\:i a"
                          }
                        ]
                      }
                    }
                  ]
                },
                {
                  "layout": "html",
                  "type": "div",
                  "cls": "tab-pane",
                  "attr": {"id": "tab-3"},
                  "schemas": [
                    {
                      "layout": "plugin",
                      "property": "/",
                      "type": "div",
                      "name": "dataList",
                      "css": { "display": "block"},
                      "plugin":{
                        "glyph": "glyphicons glyphicons-info-sign",
                        "title": "IAVMs",
                        "property": "/technology/security/vulnerabilities/cve/cve/vulnerabilities",
                        "direction": "in",
                        "limit": 50,
                        "properties":[
                          {
                            "property": "iavmNoticeNumber",
                            "linked": true
                          },
                          {
                            "property": "title"
                          },
                          {
                            "property": "releaseDate",
                            "label": "Released:",
                            "dataType": "date",
                            "format": "j\\-M\\-Y\\, g\\:i a"
                          }
                        ]
                      }
                    }
                  ]
                }
              ]
            }
          ]
        }
      ]
    }
  ]
}