{
  "name": "IAVM",
  "zones": [
    {
      "layout": "zone",
      "zone": ".menu-bar",
      "styles": ["/assets/lusidity/css/entity.css"],
      "plugins": [
        "/assets/lusidity/plugins/jquery.semanticType.js",
        "/assets/lusidity/plugins/jquery.image.js",
        "/assets/lusidity/plugins/jquery.nrt.metrics.js",
        "/assets/lusidity/plugins/jquery.dataList.js",
        "/assets/lusidity/plugins/jquery.cveViewer.js",
        "/assets/lusidity/plugins/jquery.treeViewCommon.js"
      ],
      "schemas": [
      ]
    },
    {
      "layout": "zone",
      "zone": ".panel-left",
      "css": {"padding": "0 5px 0 5px"},
      "schemas": [
        {
          "layout": "html",
          "type": "div",
          "schemas": [
            {
              "layout": "data",
              "property": "title",
              "type": "h5",
              "css": { "display": "block", "text-align": "center" }
            }
          ]
        },
        {
          "layout": "plugin",
          "property": "/",
          "type": "div",
          "cls": "cover-art center",
          "name": "image",
          "plugin":{
            "image": "/assets/img/types/iavm.png",
            "cls": "med",
            "cssFiles": [
              "/assets/lusidity/plugins/css/image.css"
            ]
          }
        },
        {
          "layout": "plugin",
          "property": "instanceOf",
          "type": "div",
          "name": "semanticType",
          "css": { "display": "block", "text-align": "center" },
          "plugin":{
            "image": false,
            "cssFiles": [
              "/assets/lusidity/plugins/css/semanticType.css"
            ]
          }
        },
        {
          "layout": "html",
          "type": "div",
          "schemas": [
            {
              "layout": "html",
              "html": "<div class=\"data-label\">Notice Id:</div>"
            },
            {
              "layout": "data",
              "property": "noticeId",
              "type": "div",
              "cls": "data-value"
            }
          ]
        },
        {
          "layout": "html",
          "type": "div",
          "schemas": [
            {
              "layout": "html",
              "html": "<div class=\"data-label\">IAVM Id:</div>"
            },
            {
              "layout": "data",
              "property": "iavmNoticeNumber",
              "type": "div",
              "cls": "data-value"
            }
          ]
        },
        {
          "layout": "html",
          "type": "div",
          "schemas": [
            {
              "layout": "html",
              "html": "<div class=\"data-label\">State:</div>"
            },
            {
              "layout": "data",
              "property": "state",
              "type": "div",
              "cls": "data-value",
              "css": {"text-transform": "uppercase"}
            }
          ]
        },
        {
          "layout": "html",
          "type": "div",
          "schemas": [
            {
              "layout": "html",
              "html": "<div class=\"data-label\">Status:</div>"
            },
            {
              "layout": "data",
              "property": "status",
              "type": "div",
              "cls": "data-value",
              "css": {"text-transform": "uppercase"}
            }
          ]
        },
        {
          "layout": "html",
          "type": "div",
          "schemas": [
            {
              "layout": "html",
              "html": "<div class=\"data-label\">Release Date:</div>"
            },
            {
              "layout": "data",
              "property": "releaseDate",
              "dataType": "date",
              "format": "j\\-M\\-Y\\",
              "type": "div",
              "cls": "data-value"
            }
          ]
        },
        {
          "layout": "html",
          "type": "div",
          "schemas": [
            {
              "layout": "html",
              "html": "<div class=\"data-label\">Acknowledged:</div>"
            },
            {
              "layout": "data",
              "property": "acknowledgeDate",
              "dataType": "date",
              "format": "j\\-M\\-Y\\",
              "type": "div",
              "cls": "data-value"
            }
          ]
        },
        {
          "layout": "html",
          "type": "div",
          "schemas": [
            {
              "layout": "html",
              "html": "<div class=\"data-label\">First Reported:</div>"
            },
            {
              "layout": "data",
              "property": "firstReportDate",
              "dataType": "date",
              "format": "j\\-M\\-Y\\",
              "type": "div",
              "cls": "data-value"
            }
          ]
        },
        {
          "layout": "html",
          "type": "div",
          "schemas": [
            {
              "layout": "html",
              "html": "<div class=\"data-label\">POAM Mitigation Date:</div>"
            },
            {
              "layout": "data",
              "property": "poamMitigationDate",
              "dataType": "date",
              "format": "j\\-M\\-Y\\",
              "type": "div",
              "cls": "data-value"
            }
          ]
        },
        {
          "layout": "html",
          "type": "div",
          "schemas": [
            {
              "layout": "html",
              "html": "<div class=\"data-label\">Suspense Date:</div>"
            },
            {
              "layout": "data",
              "property": "precoordDueDate",
              "dataType": "date",
              "format": "j\\-M\\-Y\\",
              "type": "div",
              "cls": "data-value"
            }
          ]
        },
        {
          "layout": "html",
          "type": "div",
          "schemas": [
            {
              "layout": "html",
              "html": "<div class=\"data-label\">Website:</div>"
            },
            {
              "layout": "data",
              "css": {"word-break":"break-word"},
              "property": "htmlUrl",
              "type": "div",
              "cls": "data-value"
            }
          ]
        },
        {
          "layout": "html",
          "type": "div",
          "schemas": [
            {
              "layout": "plugin",
              "property": "/",
              "type": "div",
              "name": "dataList",
              "css": { "display": "block"},
              "plugin":{
                "hasItemCounter": false,
                "glyph": "glyphicons glyphicons-article",
                "title": "Supersedes",
                "property": "/technology/security/iavm/descendants",
                "sort": {"releaseDate":"desc"},
                "limit": 10,
                "blocked": true,
                "properties":[
                  {
                    "label": "",
                    "property": "iavmNoticeNumber",
                    "linked": true
                  },
                  {
                    "label": "Release Date",
                    "property": "releaseDate",
                    "dataType": "dateOnly"
                  }
                ]
              }
            }
          ]
        },
        {
          "layout": "html",
          "type": "div",
          "schemas": [
            {
              "layout": "plugin",
              "property": "/",
              "type": "div",
              "name": "dataList",
              "css": { "display": "block"},
              "plugin":{
                "hasItemCounter": false,
                "glyph": "glyphicons glyphicons-article",
                "title": "Superseded By",
                "property": "/technology/security/iavm/descendants",
                "sort": {"releaseDate":"asc"},
                "direction": "IN",
                "limit": 10,
                "blocked": true,
                "properties":[
                  {
                    "label": "",
                    "property": "iavmNoticeNumber",
                    "linked": true
                  },
                  {
                    "label": "Release Date",
                    "property": "releaseDate",
                    "dataType": "dateOnly"
                  }
                ]
              }
            }
          ]
        }
      ]
    },
    {
      "layout": "zone",
      "zone": ".panel-middle",
      "cls": "no-border full-min",
      "attr": {"data-oh": 7},
      "schemas": [
        {
          "layout": "html",
          "type": "div",
          "schemas":[
            {
              "layout": "data",
              "property": "/",
              "type": "div",
              "schemas":[
                {
                  "property": "/system/primitives/raw_string/descriptions.results",
                  "schemas":[
                    {
                      "layout": "data",
                      "type": "p",
                      "property": "value"
                    }
                  ]
                }
              ]
            },
            {
              "layout": "html",
              "type": "ul",
              "cls": "nav nav-tabs",
              "schemas":[
                {
                  "layout": "html",
                  "type": "li",
                  "cls": "active",
                  "schemas":[
                    {
                      "layout": "html",
                      "type": "a",
                      "text": "Details",
                      "cls": "no-radius",
                      "attr": {"data-toggle": "tab", "href": "#tab-1"}
                    }
                  ]
                },
                {
                  "layout": "html",
                  "type": "li",
                  "schemas":[
                    {
                      "layout": "html",
                      "type": "a",
                      "text": "Related Security Center Plugins",
                      "cls": "no-radius",
                      "attr": {"data-toggle": "tab", "href": "#tab-2"}
                    }
                  ]
                },
                {
                  "layout": "html",
                  "type": "li",
                  "schemas":[
                    {
                      "layout": "html",
                      "type": "a",
                      "text": "Related CVEs",
                      "cls": "no-radius",
                      "attr": {"data-toggle": "tab", "href": "#tab-3"}
                    }
                  ]
                }
              ]
            },
            {
              "layout": "html",
              "type": "div",
              "cls": "tab-content",
              "css": {"padding": "0 5px 5px 5px"},
              "schemas":[
                {
                  "layout": "html",
                  "type": "div",
                  "cls": "tab-pane active",
                  "attr": {"id": "tab-1"},
                   "schemas": [
                     {
                       "layout": "html",
                       "type": "div",
                       "schemas": [
                         {
                           "layout": "html",
                           "html": "<div class=\"data-label\">Executive Summary:</div>"
                         },
                         {
                           "layout": "data",
                           "property": "executiveSummary",
                           "type": "div",
                           "cls": "data-value"
                         }
                       ]
                     },
                     {
                       "layout": "html",
                       "type": "div",
                       "schemas": [
                         {
                           "layout": "html",
                           "html": "<div class=\"data-label\">Note:</div>"
                         },
                         {
                           "layout": "data",
                           "property": "note",
                           "type": "div",
                           "cls": "data-value"
                         }
                       ]
                     },
                     {
                       "layout": "html",
                       "type": "div",
                       "schemas": [
                         {
                           "layout": "html",
                           "html": "<div class=\"data-label\">Fix Action:</div>"
                         },
                         {
                           "layout": "data",
                           "property": "fixAction",
                           "type": "div",
                           "cls": "data-value"
                         }
                       ]
                     }
                   ]
                },
                {
                  "layout": "html",
                  "type": "div",
                  "cls": "tab-pane",
                  "attr": {"id": "tab-2"},
                  "schemas": [
                    {
                      "layout": "plugin",
                      "property": "/",
                      "type": "div",
                      "name": "dataList",
                      "css": { "display": "block"},
                      "plugin":{
                        "glyph": "glyphicons glyphicons-info-sign",
                        "title": "Security Center Plugins",
                        "property": "/technology/security/iavm/iavms",
                        "direction": "in",
                        "limit": 50,
                        "properties":[
                          {
                            "property": "vulnId",
                            "linked": true
                          },
                          {
                            "property": "title"
                          },
                          {
                            "property": "published",
                            "label": "Published:",
                            "dataType": "date",
                            "format": "j\\-M\\-Y\\, g\\:i a"
                          }
                        ]
                      }
                    }
                  ]
                },
                {
                  "layout": "html",
                  "type": "div",
                  "cls": "tab-pane",
                  "attr": {"id": "tab-3"},
                  "schemas": [
                    {
                      "layout": "plugin",
                      "property": "/",
                      "type": "div",
                      "name": "dataList",
                      "css": { "display": "block"},
                      "plugin":{
                        "glyph": "glyphicons glyphicons-info-sign",
                        "title": "CVEs",
                        "property": "/technology/security/vulnerabilities/cve/cve/vulnerabilities",
                        "limit": 50,
                        "properties":[
                          {
                            "property": "vulnId",
                            "linked": true
                          },
                          {
                            "property": "published",
                            "label": "Published:",
                            "dataType": "date",
                            "format": "j\\-M\\-Y\\, g\\:i a"
                          }
                        ]
                      }
                    }
                  ]
                }
              ]
            }
          ]
        }
      ]
    }
  ]
}