{
  "name": "Location",
  "zones": [
    {
      "layout": "zone",
      "zone": ".menu-bar",
      "styles": [
        "/assets/lusidity/css/entity.css",
        "/assets/lusidity/plugins/css/tableView_v2.css"
      ],
      "plugins": [
        "/assets/lusidity/plugins/pGrid_v1.js",
        "/assets/js/chart.bundle.min.js",
        "/assets/lusidity/plugins/jquery.osIcon.js",
        "/assets/lusidity/plugins/jquery.vertexCrumb.js",
        "/assets/lusidity/plugins/jquery.semanticType.js",
        "/assets/lusidity/plugins/jquery.image.js",
        "/assets/lusidity/plugins/jquery.nrt.metrics.js",
        "/assets/lusidity/plugins/jquery.treeViewCommon.js",
        "/assets/lusidity/plugins/jquery.enclaveMenuBar.js",
        "/assets/lusidity/plugins/jquery.dataList.js",
        "/assets/js/jMouseWheel.js",
        "/assets/lusidity/plugins/pVulnAssets.js",
        "/assets/lusidity/plugins/pSummary.js"
      ],
      "schemas": [
      ]
    },
    {
      "layout": "zone",
      "zone": ".panel-left",
      "css": {"padding": "0 5px 0 5px"},
      "schemas": [
        {
          "layout": "html",
          "type": "div",
          "schemas": [
            {
              "layout": "data",
              "property": "title",
              "type": "h5",
              "css": { "display": "block", "text-align": "center" }
            }
          ]
        },
        {
          "layout": "plugin",
          "property": "/",
          "type": "div",
          "cls": "cover-art center",
          "name": "image",
          "plugin":{
            "image": "/assets/img/types/location.png",
            "cls": "med",
            "cssFiles": [
              "/assets/lusidity/plugins/css/image.css"
            ]
          }
        },
        {
          "layout": "plugin",
          "property": "instanceOf",
          "type": "div",
          "name": "semanticType",
          "css": { "display": "block", "text-align": "center" },
          "plugin":{
            "image": false,
            "cssFiles": [
              "/assets/lusidity/plugins/css/semanticType.css"
            ]
          }
        },
        {
          "layout": "html",
          "type": "div",
          "schemas": [
            {
              "layout": "html",
              "html": "<div class=\"data-label\">Abbreviation:</div>"
            },
            {
              "layout": "data",
              "property": "abbreviation",
              "maxItems": 1,
              "cls": "data-value"
            }
          ]
        }
      ]
    },
    {
      "layout": "zone",
      "zone": ".panel-middle",
      "cls": "no-border full-min",
      "attr": {"data-oh": 7},
      "schemas": [
        {
          "layout": "html",
          "type": "div",
          "schemas":[
            {
              "layout": "data",
              "property": "/",
              "type": "div",
              "schemas":[
                {
                  "property": "/system/primitives/raw_string/descriptions.results",
                  "schemas":[
                    {
                      "layout": "data",
                      "type": "p",
                      "property": "value"
                    }
                  ]
                }
              ]
            },
            {
              "layout": "html",
              "type": "ul",
              "cls": "nav nav-tabs",
              "schemas":[
                {
                  "layout": "html",
                  "type": "li",
                  "cls": "active",
                  "schemas":[
                    {
                      "layout": "html",
                      "type": "a",
                      "text": "Asset Summary",
                      "cls": "no-radius",
                      "attr": {"data-toggle": "tab", "href": "#tab-mitigations"}
                    }
                  ]
                }
              ]
            },
            {
              "layout": "html",
              "type": "div",
              "cls": "tab-content",
              "css": {"padding": "0 5px 5px 5px"},
              "schemas":[
                {
                  "layout": "html",
                  "type": "div",
                  "cls": "tab-pane active",
                  "attr": {"id": "tab-mitigations"},
                  "schemas": [
                    {
                      "layout": "plugin",
                      "property": "/",
                      "type": "div",
                      "name": "pSummary",
                      "css": { "display": "block"},
                      "plugin":{
                        "adjustHeight": 14
                      }
                    }
                  ]
                }
              ]
            }
          ]
        }
      ]
    }
  ]
}